{
  "commands": {
    "build": {
      "fileName": "cmd.exe",
      "workingDirectory": ".",
      "arguments": "/c build.bat"
    },
    "deploy": {
      "fileName": "cmd.exe",
      "workingDirectory": ".",
      "arguments": "/c deploy.bat"
    }
  },
  "-vs-binding": { "AfterBuild": [ "build", "deploy" ] }
}